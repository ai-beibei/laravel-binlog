{
    "name": "telanflow/laravel-binlog",
    "description": "mysql binlog for laravel",
    "keywords": [
        "binlog",
        "swoole",
        "laravel",
        "lumen",
        "mysql"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "telanflow",
            "email": "telanflow@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-posix": "*",
        "ext-pcntl": "*",
        "ext-bcmath": "*",
        "ext-swoole": "^4.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.4",
        "phpunit/phpunit": "^8.4",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Telanflow\\Binlog\\": "src/"
        },
        "files": [
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Telanflow\\Binlog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Telanflow\\Binlog\\LaravelServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    ]
}
